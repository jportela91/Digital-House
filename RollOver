<!DOCTYPE html>
<html>
  <head>
        <script src="sketch.js"></script>
        <script src="http://cdnjs.cloudflare.com/ajax/libs/p5.js/0.5.6/p5.js">
        </script>
        <title></title>
      <script>
      var x=50;
      var y=50;
      var w=50;
      var h=50;

      function setup(){
        createCanvas(200,200)
      }
      function draw(){
        background(255);
        stroke(0);
        if(mouseX>=x && mouseY>=y && mouseY<=y+h && mouseX <=x+w){
        fill("red")
        }else{
        fill("blue")
        }
        rect(x,y,w,h);
      }


      </script>

  </head>
  <body>

  </body>
</html>
