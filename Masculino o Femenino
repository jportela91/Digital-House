  var m
  var f
  var dato = prompt("Escribi tu Genero..","")
  var m = "masculino"
  var f = "femenino"

  function setup(){
    createCanvas(1240,1240)
  }
  function draw(){

      if(dato == m){
        triangle(300,200,150,400,450,400)
      }else{
        
          triangle(300,600,150,400,450,400)
      }

}





